grammar org.xtext.example.mydsl.MyDsl with org.eclipse.xtext.common.Terminals

generate myDsl "http://www.xtext.org/example/mydsl/MyDsl"

Model:
	contract=Contract;
	
Contract:
	'Contrat' '{' 	Recipient ';'
					Client ';' 
					Mediator ';' 
					Payment ';'
					
					'Date' ':' date=STRING ';'
					'Ville' ':' town=STRING ';'
	'}'
;

Recipient:
	'Prestataire' '{' '}'
;


Client:
	'Client' '{' '}'
;

Mediator:
	'Mediateur' '{' '}'
;

Payment:
	'Paiement' '{'
		'Prix' ':' prix=INT
	'}'	
;